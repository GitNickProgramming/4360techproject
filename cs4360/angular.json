{
  "version": 1,
  "projects": {
    "crm-ui": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/crm/ui",
      "sourceRoot": "apps/crm/ui/src",
      "prefix": "cs4360",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/crm/ui",
            "index": "apps/crm/ui/src/index.html",
            "main": "apps/crm/ui/src/main.ts",
            "polyfills": "apps/crm/ui/src/polyfills.ts",
            "tsConfig": "apps/crm/ui/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/crm/ui/src/favicon.ico", "apps/crm/ui/src/assets"],
            "styles": ["apps/crm/ui/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/crm/ui/src/environments/environment.ts",
                  "with": "apps/crm/ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "crm-ui:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "crm-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "crm-ui:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/crm/ui/tsconfig.app.json",
              "apps/crm/ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/crm/ui/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/crm/ui/jest.config.js",
            "tsConfig": "apps/crm/ui/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/crm/ui/src/test-setup.ts"
          }
        }
      }
    },
    "crm/ui-e2e": {
      "root": "apps/crm/ui-e2e",
      "sourceRoot": "apps/crm/ui-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/crm/ui-e2e/cypress.json",
            "tsConfig": "apps/crm/ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "crm-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "crm-ui:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/crm/ui-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/crm/ui-e2e/**/*"]
          }
        }
      }
    },
    "crm-api": {
      "root": "apps/crm/api",
      "sourceRoot": "apps/crm/api/src",
      "projectType": "application",
      "prefix": "crm-api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/crm/api",
            "main": "apps/crm/api/src/main.ts",
            "tsConfig": "apps/crm/api/tsconfig.app.json",
            "assets": ["apps/crm/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/crm/api/src/environments/environment.ts",
                  "with": "apps/crm/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "crm-api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/crm/api/tsconfig.app.json",
              "apps/crm/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/crm/api/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/crm/api/jest.config.js",
            "tsConfig": "apps/crm/api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "crm-nest-dao": {
      "root": "libs/crm/nest/dao",
      "sourceRoot": "libs/crm/nest/dao/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/crm/nest/dao/tsconfig.lib.json",
              "libs/crm/nest/dao/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/crm/nest/dao/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/crm/nest/dao/jest.config.js",
            "tsConfig": "libs/crm/nest/dao/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "timmy-crm"
}
