{
  "version": 1,
  "projects": {
    "timmy-crm": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/timmy-crm",
      "sourceRoot": "apps/timmy-crm/src",
      "prefix": "cs4360",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/timmy-crm",
            "index": "apps/timmy-crm/src/index.html",
            "main": "apps/timmy-crm/src/main.ts",
            "polyfills": "apps/timmy-crm/src/polyfills.ts",
            "tsConfig": "apps/timmy-crm/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/timmy-crm/src/favicon.ico",
              "apps/timmy-crm/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/timmy-crm/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/timmy-crm/src/environments/environment.ts",
                  "with": "apps/timmy-crm/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "timmy-crm:build",
            "proxyConfig": "apps/timmy-crm/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "timmy-crm:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "timmy-crm:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/timmy-crm/tsconfig.app.json",
              "apps/timmy-crm/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/timmy-crm/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/timmy-crm/jest.config.js",
            "tsConfig": "apps/timmy-crm/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/timmy-crm/src/test-setup.ts"
          }
        }
      }
    },
    "timmy-crm-e2e": {
      "root": "apps/timmy-crm-e2e",
      "sourceRoot": "apps/timmy-crm-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/timmy-crm-e2e/cypress.json",
            "tsConfig": "apps/timmy-crm-e2e/tsconfig.e2e.json",
            "devServerTarget": "timmy-crm:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "timmy-crm:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/timmy-crm-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/timmy-crm-e2e/**/*"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
